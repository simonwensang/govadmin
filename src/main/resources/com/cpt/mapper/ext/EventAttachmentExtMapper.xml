<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cpt.mapper.ext.EventAttachmentExtMapper">
  <resultMap id="BaseResultMap" type="com.cpt.model.EventAttachment">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="event_id" jdbcType="INTEGER" property="eventId" />
    <result column="attachment" jdbcType="VARCHAR" property="attachment" />
    <result column="attachment_name" jdbcType="VARCHAR" property="attachmentName" />
  </resultMap>

  <insert id="insertBatch" parameterType="com.cpt.model.EventAttachment">
    insert into t_event_attachment ( event_id, attachment,
      attachment_name)
    values
    <foreach collection="list" item="item" index='index' separator="," >
      (#{item.eventId,jdbcType=INTEGER}, #{item.attachment,jdbcType=VARCHAR},
      #{item.attachmentName,jdbcType=VARCHAR})
    </foreach>
  </insert>

</mapper>