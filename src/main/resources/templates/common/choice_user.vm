<div id="modal-table" class="modal fade select-p-wrap" tabindex="-1" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header no-padding">
                <div class="table-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <span class="white">×</span>
                    </button>
                    <span id="model_title">选择审批人</span>
                </div>
            </div>

            <div class="modal-body no-padding">
                <div class="form-group" style="margin-top: 10px;margin-bottom: 10px">
                    <input type="text" placeholder="姓名" id="sname" style="margin-left: 20px;vertical-align: middle;">
                    <button class="btn btn-sm btn-primary" style="height: 30px">筛选</button>

                    <div class="pull-right" style="margin-right: 84px;margin-top: 10px;">
                        <label>
                            <input id="select_all" name="form-field-checkbox" type="checkbox" class="ace" />
                            <span class="lbl">全选</span>
                        </label>
                    </div>

                </div>
                <table class="table table-striped table-bordered table-hover no-margin-bottom no-border-top">
                    <thead>
                    <tr>
                        <th>ID</th>
                        <th>姓名</th>
                        <th>级别</th>
                        <th>操作</th>
                    </tr>
                    </thead>
                    <tbody id="tbody">
                    </tbody>
                </table>
            </div>

            <div class="modal-footer no-margin-top">
                <button class="btn btn-sm btn-danger pull-left" data-dismiss="modal">
                    <i class="icon-remove"></i>
                    取消
                </button>

                <button class="btn btn-sm btn-primary pull-left js-ok-1">
                    <i class="icon-remove"></i>
                    确定
                </button>

                <ul id="pager" class="pagination pull-right no-margin">

                </ul>
            </div>
        </div>
    </div>
</div>
                        